			"no-api": true
		},
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-convert",
			"no-api": true
		},
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-assert",
			"no-api": true
		}

	],
	"name": "plibv4/input",
